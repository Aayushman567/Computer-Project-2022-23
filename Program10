public class TechNumber {

	public static void main(String[] args) {
		
		int digits = 0, firstPart, secondPart, n;
		int square;
		
		for(int num = 1000; num < 10000; num++) {
			
			firstPart = num % 100;
			secondPart = num / 100;
			square = (firstPart + secondPart) * (firstPart + secondPart);
			
			
			if(num == square) {
					System.out.println("Tech Number: " + num);
			}
			
		}
		
	}

}
